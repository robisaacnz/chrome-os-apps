{
    "manifest_version": 2,
    "name": "Photos",
    "description": "Access to photos in your Apple iCloud account.",
    "version": "1.0",
    "icons": {
        "128": "img/icon.png"
    },
    "app": {
        "background": {
            "scripts": [
                "js/background.js"
            ],
            "pages": [
                "html/embed.html"
            ]
        }
    },
    "permissions": [
        "webview",
        "fullscreen"
    ]
}